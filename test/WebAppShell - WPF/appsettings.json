{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "AllowedHosts": "127.0.0.1",
    "Kestrel": {
        "Endpoints": {
            "HTTP": {
                "Url": "http://127.0.0.1:0"
            }
        }
    },
    "ContentTypes": {
        ".glb": "model/gltf-binary",
        ".gltf": "model/gltf-json",
        ".vtt": "text/vtt",
        ".webmanifest": "application/manifest+json"
    },
    "DefaultFiles": [
        "index.html"
    ],
    "UseWebSockets": true
}