{
  "AllowedHosts": "*",
  "Database": "local",
  "Import": "none",
  "UseCookies": false,
  "UseSession": true,
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Error",
      "Microsoft.EntityFrameworkCore.Database.Command": "Error"
    }
  },
  "ContentTypes": {
    ".geojson": "application/geo+json",
    ".glb": "model/gltf-binary",
    ".gltf": "model/gltf-json",
    ".vert": "x-shader/x-vertex",
    ".frag": "x-shader/x-fragment",
    ".glsl": "x-shader/x-glsl",
    ".wgsl": "text/wgsl",
    ".kml": "application/vnd.google-earth.kml+xml",
    ".kmz": "vnd.google-earth.kmz",
    ".vtt": "text/vtt",
    ".webmanifest": "application/manifest+json"
  },
  "DefaultFiles": [
    "index.html"
  ],
  "ConnectionStrings": {
    "Sqlite": "Data Source=Sqlite/CedrusExample.db",
    "Postgres": "Host=localhost; Database=cedrus; Username=postgres; Password=postgres",
    "CedrusContextInsecureConnection": "Data Source=Example.db"
  },
  "AppShell": {
    "Window": {
      "Size": "800x600"
    }
  },
  "Version": "0.0.1"
}